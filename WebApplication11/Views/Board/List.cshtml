@model List<WebApplication11.Models.Document>

@*
   레이저라는 문법이 있다 훨씬 더 간단함
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
    Layout = null;
    var member = ViewData["Member"] as WebApplication11.Models.Member;
}

<html xmlns="http://www.w3.org/1999/xhtml">
    <head
        ><title></title>
    </head>
    <body>
        <div>
            <div>
                @member.Name 님 환영합니다.

            </div>
            <table>
                <thead>
                    <tr>
                        <th>번호</th>
                        <th>제목</th>
                        <th>글쓴이</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach(var item in Model)
                    {
                    

                    <tr>
                        <td>@item.Document_Number</td>
                         <td>@item.Title</td>
                         <td>@item.Writer</td>
                    </tr>
                    }
                </tbody>
            </table>
        </div>

    </body>
</html>
